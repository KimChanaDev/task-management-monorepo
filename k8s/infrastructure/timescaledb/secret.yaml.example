# ============================================
# TimescaleDB Secret Template
# ============================================
# Copy this file to secret.yaml and fill in the values
# cp secret.yaml.example secret.yaml

apiVersion: v1
kind: Secret
metadata:
  name: timescaledb-secret
  namespace: task-platform-infra
type: Opaque
stringData:
  username: <USERNAME>
  password: <PASSWORD>
  database: <DATABASE>
