{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": ["generate-ts-proto", "prisma:generate", "prisma:migrate", "^build"],
      "cache": false,
      "persistent": true
    },
    "prisma:generate": {
      "dependsOn": ["^prisma:generate"]
    },
    "prisma:migrate": {
      "dependsOn": ["^prisma:migrate"]
    },
    "generate-ts-proto": {
      "dependsOn": ["^generate-ts-proto"]
    }
  }
}
